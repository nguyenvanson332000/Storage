<% provide(:title, "booking.name") %>
<% provide(:base_title, "base.title") %>
<div class="col-lg-9 box-booking">
  <div class="head">
    <h3>booking_name</h3>
  </div>
  <div class="content">
    <table class="table-booking">
      <thead>
        <tr>
          <th scope="col"></th>
          <th scope="col"></th>
          <th scope="col"> booking_yard_name </th>
          <th scope="col"> booking_yard_code</th>
          <th scope="col"> booking_yard_type</th>
          <th scope="col"> detail_time</th>
          <th scope="col"> detail_date</th>
          <th scope="col"> booking_price</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row">
            <span> booking_delete </span>
          </th>
          <td>
            <div class="box-img">
              <img src="" alt="Not img">
            </div>
          </td>
          <td> Yard A </td>
          <td> JAVA </td>
          <td> 5 </td>
          <td> 17:00 </td>
          <td> 10/11/2020 </td>
          <td> 10$ </td>
        </tr>
        <tr>
          <th scope="row">
            <span> booking_delete </span>
          </th>
          <td>
            <div class="box-img">
              <img src="" alt="Not img">
            </div>
          </td>
          <td> Yard B</td>
          <td> JAVB </td>
          <td> 5 </td>
          <td> 17:00 </td>
          <td> 10/11/2020 </td>
          <td> 10$ </td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="footer-booking">
    <div class="total">
      <p><b> booking_total : </b><span> 20$ </span></p>
    </div>
    <div class="booking">
      <button type="button" class="btn btn-primary mr-3">booking_name</button>
      <button type="button" class="btn btn-secondary"> cancel </button>
    </div>
  </div>
</div>
